{
  "name": "nestled",
  "version": "1.2.0",
  "description": "Javascript NES emulator",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./src/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf dist lib",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir lib --source-maps",
    "build:bundle": "BABEL_ENV=esm rollup -c",
    "build:types": "tsc",
    "build": "npm run build:cjs && npm run build:bundle",
    "test": "mocha \"test/**/*_test.js\" || true",
    "debug": "mocha debug \"test/**/*_test.js\" || true",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "echo \"Not meant to be published as package...\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jtduchesne/Nestled.git"
  },
  "keywords": [
    "Javascript",
    "ES6",
    "NES",
    "Nintendo",
    "8bit",
    "8-bit",
    "emulator"
  ],
  "author": "jtduchesne",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jtduchesne/Nestled/issues"
  },
  "homepage": "https://github.com/jtduchesne/Nestled#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@rollup/plugin-babel": "^5.3.1",
    "bdd-lazy-var": "^2.6.1",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "mocha": "^9.1.3",
    "rollup": "^2.77.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.7.4"
  }
}
